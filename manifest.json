{
    "name": "挂机助手",
    "description": "自动挂机，避免检测",
    "manifest_version": 2,
    "version": "1.0",
    "permissions": [
        "*://*/*",
        "contextMenus",
        "webRequest",
        "webRequestBlocking",
        "identity"
    ],
    "background": {
        "scripts": [
            "/background.js"
        ]
    }
}